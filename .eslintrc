{
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
  },
  "plugins": ["svelte3"],
  "rules": {
    "space-before-function-paren": ["error", "never"],
  },
  "overrides": [
    {
      "files": ["**/*.svelte"],
      "processor": "svelte3/svelte3",
      "rules": {
        "import/imports-first": "off",
        "import/no-unresolved": "off",
        "import/no-mutable-exports": "off",
        "import/no-duplicates": "off",
      }
    }
  ],
}